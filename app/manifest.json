{
  "manifest_version": 2,
  "name": "tabbano",
  "description": "Tab annotator and tab organizer",
  "version": "0.9",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html",
    "default_title": "Tabanno - tab organizer"
  },
  "permissions": ["activeTab", "tabs"]
}
