{
  "manifest_version": 2,
  "name": "tabbano",
  "description": "Tab annotator and tab organizer",
  "version": "0.9",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html",
    "default_title": "Tabanno - tab organizer"
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "scripts": ["thirdParty/jquery-2.0.3.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://website*"],
      "js": ["thirdParty/jquery.1.10.2.min.js", "script.js"],
      "css": ["css/style.css"],
      "run_at": "document_end"
    }
  ]
}
